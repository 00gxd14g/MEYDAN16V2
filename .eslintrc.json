{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": ["warn", { "allow": ["warn", "error", "info", "log"] }],
    "semi": ["error", "always"],
    "quotes": ["warn", "single", { "allowTemplateLiterals": true }],
    "indent": ["warn", 4, { "SwitchCase": 1 }],
    "no-undef": "error",
    "no-redeclare": "error",
    "no-dupe-keys": "error",
    "no-duplicate-case": "error",
    "no-unreachable": "warn",
    "no-empty": "warn",
    "prefer-const": "warn",
    "no-var": "warn",
    "eqeqeq": ["warn", "always"],
    "curly": ["warn", "all"],
    "brace-style": ["warn", "1tbs"],
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-with": "error",
    "no-debugger": "warn",
    "no-alert": "warn",
    "prefer-arrow-callback": "off",
    "func-style": "off"
  },
  "globals": {
    "updateSubCategory": "writable",
    "navigateToSection": "writable"
  }
}
